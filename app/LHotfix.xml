<?xml version="1.0" encoding="utf-8"?>
<resources>

  <!--需要热补的包名或者类名，这些包名下的所有类都被会插入代码-->
  <hotfixPackage>
    <name>com.beggar.lhotfix.demo</name>
    <name>com.beggar.lhotfix.demo2</name>
  </hotfixPackage>

  <!--不需要Robust插入代码的包名，Robust库不需要插入代码，如下的配置项请保留，还可以根据各个APP的情况执行添加-->
  <exceptPackage>
    <name>com.beggar.lhotfix.demo1</name>
  </exceptPackage>

  <!--  补丁类的包名  -->
  <patchPackName name = "patchPackName">
    <name>com.beggar.hotfix.patch</name>
  </patchPackName>

</resources>
